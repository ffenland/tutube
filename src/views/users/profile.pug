extends ../base 
include ../mixins/video

block content
	div.profile__data
		if user.avatarUrl === ""
			div.initial
				span #{user.email[0].toUpperCase()}
		else
			img(src=user.avatarUrl).avatar
		h3=user.email
	div.video-grid
		each video in user.videos
			+video(video)
		else
			span.empty__message No videos found